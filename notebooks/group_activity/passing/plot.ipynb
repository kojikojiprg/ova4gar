    "        hist_room = []\n",
    "        for key, x_lst, y_lst in zip(x_dict.keys(), x_dict.values(), y_dict.values()):\n",
    "            if room_num in key and is_pass in y_lst:\n",
    "                hist_room += x_lst.T[col_num].tolist()\n",
    "        hist[room_num] = hist_room\n",
    "        \n",
    "    return hist"
   ]
